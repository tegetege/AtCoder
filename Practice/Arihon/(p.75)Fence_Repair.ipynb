   "source": [
    "import heapq\n",
    "\n",
    "que = []\n",
    "\n",
    "for i in range(len(L)):\n",
    "    heapq.heappush(que, L[i])\n",
    "\n",
    "cost = 0\n",
    "while len(que) > 1:\n",
    "    l1 = heapq.heappop(que)\n",
    "    l2 = heapq.heappop(que)\n",
    "    cost += l1 + l2\n",
    "    heapq.heappush(que, (l1+l2))\n",
    "    \n",
    "print(cost)"
   ]
